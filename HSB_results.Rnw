}
f_lmp <- function(Re_name) {
  rebound_analysis_with_cal_k %>% 
    dplyr::filter(.data[["Case"]] == "Lamp") %>% 
    magrittr::extract2(Re_name) %>% 
    magrittr::multiply_by(100) %>% 
